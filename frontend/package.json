{
  "name": "a-minimal-blog-starter",
  "description": "a mininmal blog starter built with strapi and sapper",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "npm run build:tailwindcss && sapper export",
    "export": "npm run build:tailwindcss && sapper export",
    "dev:tailwindcss": "postcss static/tailwind.css -o static/main.css -w",
    "build:tailwindcss": "NODE_ENV=production postcss static/tailwind.css -o static/main.css",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^3.1.3",
    "apollo-boost": "^0.4.7",
    "axios": "^0.21.4",
    "body-parser": "^1.19.0",
    "compression": "^1.7.1",
    "express-session": "^1.17.1",
    "graphql": "^15.0.0",
    "moment": "^2.29.4",
    "node-fetch": "^2.6.9",
    "polka": "next",
    "session-file-store": "^1.5.0",
    "sirv": "^0.4.0",
    "svelte-pdf": "^1.0.12",
    "whatsapp-cloud-api": "^0.2.6"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1",
    "rollup": "^1.20.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^5.3.0",
    "sapper": "^0.27.0",
    "svelte": "^3.55.1",
    "tailwindcss": "^2.1.2"
  }
}
